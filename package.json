{
  "name": "@d3p1/thr2pxl",
  "version": "1.11.3",
  "description": "A 3D (thr) model to pixel transformation with motion effect",
  "private": false,
  "type": "module",
  "main": "./dist/core/index.js",
  "module": "./dist/core/index.js",
  "types": "./dist/core/index.d.ts",
  "exports": {
    "./core": {
      "import": {
        "types": "./dist/core/index.d.ts",
        "default": "./dist/core/index.js"
      }
    },
    "./react": {
      "import": {
        "types": "./dist/react/index.d.ts",
        "default": "./dist/react/index.js"
      }
    }
  },
  "files": [
    "./dist"
  ],
  "scripts": {
    "prepare": "husky",
    "lint": "npx eslint src",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "npx prettier src --check",
    "prettier:fix": "npm run prettier -- --write",
    "format": "npm run prettier:fix && npm run lint:fix",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/d3p1/thr2pxl.git"
  },
  "keywords": [
    "javascript",
    "webgl",
    "threejs",
    "typescript",
    "html5",
    "canvas",
    "creative-coding",
    "particles",
    "gpgpu",
    "particle",
    "particle-physics",
    "webgl2",
    "three",
    "particles-js",
    "gpgpu-computing"
  ],
  "author": "C. M. de Picciotto <d3p1@d3p1.dev> (https://d3p1.dev/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/d3p1/thr2pxl/issues"
  },
  "homepage": "https://github.com/d3p1/thr2pxl#readme",
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@d3p1/commitlint-config": "^1.0.0",
    "@eslint/js": "^9.39.1",
    "@tailwindcss/vite": "^4.1.18",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react-swc": "^4.2.2",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "husky": "^8.0.3",
    "prettier": "3.0.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwindcss": "^4.1.18",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4",
    "vite-plugin-dts": "^4.5.3",
    "write-good": "^1.0.8",
    "@tweakpane/core": "^2.0.5",
    "@types/three": "^0.174.0",
    "three": "^0.173.0",
    "tweakpane": "^4.0.5",
    "vite-plugin-glsl": "^1.5.5",
    "@gltf-transform/cli": "^4.1.4",
    "@gltf-transform/functions": "^4.1.4"
  },
  "peerDependencies": {
    "three": "^0.176.0",
    "tweakpane": "^4.0.5",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
